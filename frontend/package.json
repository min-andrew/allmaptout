{
  "name": "wedding-frontend",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "astro dev",
    "dev:check": "concurrently \"astro dev\" \"astro check --watch\"",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "lint": "eslint src",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "typecheck": "astro check",
    "typecheck:watch": "astro check --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate": "kubb"
  },
  "dependencies": {
    "@astrojs/svelte": "^7.2.4",
    "@astrojs/tailwind": "^6.0.2",
    "@tanstack/svelte-query": "^6.0.14",
    "astro": "^5.16.6",
    "axios": "^1.13.2",
    "svelte": "^5.46.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@eslint/js": "^9.39.2",
    "@kubb/cli": "^4.12.14",
    "@kubb/core": "^4.12.14",
    "@kubb/plugin-client": "^4.12.15",
    "@kubb/plugin-oas": "^4.12.14",
    "@kubb/plugin-svelte-query": "^4.12.14",
    "@kubb/plugin-ts": "^4.12.14",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@typescript-eslint/parser": "^8.51.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-svelte": "^3.13.1",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  }
}
